<script>
  var filter_params = <%= raw filter_params.to_json %>;
</script>

<%= form_tag report_path(name: report.class.name), method: :get,  class: 'form-inline' do %>
  <% report.filters.each do |filter| %>
    <div class='form-group'>
      <%= label_tag filter[:name], nil, class: 'control-label'  %>
      <%= filter_tag filter, report, class: 'form-control' %>
    </div>
  <% end %>
  <div class='form-group'>
    <%= submit_tag 'Filter', class: 'btn btn-success' %>
  </div>
<% end %>
